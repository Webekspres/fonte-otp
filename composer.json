{
    "name": "webekspres/fonte-otp",
    "description": "Laravel package to send WhatsApp OTP via Fonnte API, supporting Laravel 8â€“12.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Webekspres",
            "email": "info@webekspres.com"
        },
        {
            "name": "Fadhila36",
            "homepage": "https://github.com/Fadhila36"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "pestphp/pest": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Webekspres\\FonnteOtp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webekspres\\FonnteOtp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webekspres\\FonnteOtp\\Providers\\FonnteOtpServiceProvider"
            ],
            "aliases": {
                "FonnteOtp": "Webekspres\\FonnteOtp\\Facades\\FonnteOtp"
            }
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Webekspres\\FonnteOtp\\Setup\\EnvSetup::handle"
        ],
        "post-update-cmd": [
            "Webekspres\\FonnteOtp\\Setup\\EnvSetup::handle"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}